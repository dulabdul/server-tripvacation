<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
    <div class="card shadow mb-4">
        <div class="card-body">
            <div class="row">
                <% for (let i = 0; i < item.imageId.length; i++) { %>
                <div class="col-auto">
                    <img src="https://tripvacation-admin.herokuapp.com/<%= item.imageId[i].imageUrl %>" width="500"
                        class="img-fluid d-block" alt="Image">
                </div>
                <% } %>
            </div>
            <a href="/admin/item" class="btn btn-warning mt-4"><i class="fas fa-arrow-left"></i> Back</a>
        </div>
    </div>
</div>